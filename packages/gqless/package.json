{
  "name": "@dish/gqless",
  "version": "3.0.13",
  "description": "",
  "license": "MIT",
  "author": "PabloSz",
  "main": "dist/index.js",
  "module": "dist/gqless.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "rimraf node_modules/.cache && tsdx build",
    "dev": "nodemon",
    "dev:nodemon": "tsup src -d dist && node dist/QueryBuilder/buildQuery.js",
    "lint": "tsdx lint",
    "playground": "tsdx test test/playground.test.ts --watch --no-coverage",
    "prepare": "rimraf node_modules/.cache && tsdx build",
    "size": "size-limit",
    "start": "tsc-watch -m commonjs --noClear",
    "test": "tsdx test",
    "test:watch": "tsdx test --watch",
    "test:watch-coverage": "rimraf coverage && mkdirp coverage/lcov-report && concurrently --raw \"tsdx test --watchAll\" \"serve -l 8787 coverage/lcov-report\" \"wait-on tcp:8787 coverage/lcov-report/index.html && open-cli http://localhost:8787\""
  },
  "devDependencies": {
    "@dish/gqless-cli": "workspace:^4.0.4",
    "@size-limit/preset-small-lib": "^4.10.1",
    "@types/concurrently": "^6.0.1",
    "@types/lodash": "^4.14.168",
    "@types/mkdirp": "^1.0.1",
    "@types/node": "^14.14.33",
    "@types/nodemon": "^1.19.0",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/rimraf": "^3.0.0",
    "@types/tap": "^14.10.3",
    "@types/tsc-watch": "^4.2.0",
    "@types/wait-on": "^5.2.0",
    "concurrently": "^6.0.0",
    "graphql": "^15.5.0",
    "mercurius": "^7.2.0",
    "mkdirp": "^1.0.4",
    "nodemon": "^2.0.7",
    "open-cli": "^6.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rimraf": "^3.0.2",
    "serve": "^11.3.2",
    "size-limit": "^4.10.1",
    "tap": "^14.11.0",
    "test-utils": "workspace:^0.1.0",
    "tsc-watch": "^4.2.9",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "tsup": "^4.6.1",
    "typescript": "^4.2.3",
    "wait-on": "^5.2.1"
  },
  "peerDependencies": {
    "graphql": "^15.4.0"
  },
  "size-limit": [
    {
      "path": "dist/gqless.cjs.production.min.js",
      "limit": "20 KB"
    },
    {
      "path": "dist/gqless.esm.js",
      "limit": "20 KB"
    }
  ]
}
