  "description": "",
  "license": "MIT",
  "author": "PabloSz",
  "main": "dist/index.js",
  "module": "dist/gqless-tsdx.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "rimraf node_modules/.cache && tsdx build",
    "dev": "nodemon",
    "dev:nodemon": "tsup src -d dist && node dist/QueryBuilder/buildQuery.js",
    "lint": "tsdx lint",
    "playground": "tsdx test test/playground.test.ts --watch --no-coverage",
    "prepare": "rimraf node_modules/.cache && tsdx build",
    "size": "size-limit",
    "start": "tsc-watch -m commonjs --noClear",
    "test": "tsdx test",
    "test:watch": "tsdx test --watch"
  },
  "dependencies": {
    "lodash": "^4.17.20",
    "lodash-es": "^4.17.20"
  },
  "devDependencies": {
    "@dish/gqless-cli": "workspace:^4.0.0",
    "@size-limit/preset-small-lib": "^4.9.2",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.25",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@types/tap": "^14.10.2",
    "graphql": "^15.5.0",
    "mercurius": "^6.12.0",
    "nodemon": "^2.0.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rimraf": "^3.0.2",
    "size-limit": "^4.9.2",
    "tap": "^14.11.0",
    "test-utils": "workspace:^0.1.0",
    "tsc-watch": "^4.2.9",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "tsup": "^3.12.1",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "graphql": "^15.4.0"
  },
  "size-limit": [
    {
      "path": "dist/gqless.cjs.production.min.js",
      "limit": "20 KB"
    },
    {
      "path": "dist/gqless.esm.js",
      "limit": "20 KB"
    }
  ]
