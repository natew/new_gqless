{
  "name": "@dish/gqless-cli",
  "version": "4.0.4",
  "license": "MIT",
  "author": "PabloSz",
  "main": "dist/index.js",
  "module": "dist/gqless-cli.esm.js",
  "typings": "dist/index.d.ts",
  "bin": {
    "gqless": "bin/gqless.js"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "tsdx build",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "start": "tsdx watch --verbose --noClean",
    "test": "tsdx test",
    "test:watch": "tsdx test --watch",
    "test:watch-coverage": "rimraf coverage && mkdirp coverage/lcov-report && concurrently --raw \"tsdx test --watchAll\" \"serve -l 8787 coverage/lcov-report\" \"wait-on tcp:8787 coverage/lcov-report/index.html && open-cli http://localhost:8787\""
  },
  "dependencies": {
    "@graphql-codegen/core": "^1.17.9",
    "@graphql-codegen/typescript": "^1.21.0",
    "@graphql-tools/delegate": "^7.0.10",
    "@graphql-tools/utils": "^7.5.0",
    "@graphql-tools/wrap": "^7.0.5",
    "commander": "^7.1.0",
    "cosmiconfig": "^7.0.0",
    "cross-fetch": "^3.0.6",
    "graphql": "^15.5.0",
    "mkdirp": "^1.0.4",
    "prettier": "^2.2.1",
    "runtypes": "^5.0.1"
  },
  "devDependencies": {
    "@dish/gqless": "workspace:^3.0.13",
    "@dish/gqless-cli": "workspace:^4.0.4",
    "@size-limit/preset-small-lib": "^4.10.0",
    "@types/mkdirp": "^1.0.1",
    "open-cli": "^6.0.1",
    "rimraf": "^3.0.2",
    "serve": "^11.3.2",
    "size-limit": "^4.10.0",
    "test-utils": "workspace:^0.1.0",
    "tmp-promise": "^3.0.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3",
    "wait-on": "^5.2.1"
  },
  "peerDependencies": {
    "@dish/gqless": "workspace:^3.0.0"
  },
  "engines": {
    "node": ">=10"
  },
  "size-limit": [
    {
      "path": "dist/gqless-cli.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/gqless-cli.esm.js",
      "limit": "10 KB"
    }
  ]
}
