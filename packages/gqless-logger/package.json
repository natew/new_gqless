{
  "name": "@dish/gqless-logger",
  "version": "0.1.3",
  "license": "MIT",
  "author": "PabloSz",
  "main": "dist/index.js",
  "module": "dist/gqless-logger.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "rimraf node_modules/.cache && tsdx build",
    "prepare": "rimraf node_modules/.cache && tsdx build",
    "size": "size-limit",
    "start": "tsdx watch --noClean --verbose",
    "test": "tsdx test",
    "test:watch": "tsdx test --watch"
  },
  "dependencies": {
    "prettier": "^2.2.1"
  },
  "devDependencies": {
    "@dish/gqless": "workspace:^3.0.8",
    "@size-limit/preset-small-lib": "^4.9.2",
    "@types/prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "size-limit": "^4.9.2",
    "test-utils": "workspace:^0.1.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.2"
  },
  "peerDependencies": {
    "@dish/gqless": "workspace:^1.0.22"
  },
  "engines": {
    "node": ">=10"
  },
  "size-limit": [
    {
      "path": "dist/gqless-logger.cjs.production.min.js",
      "limit": "260 KB"
    },
    {
      "path": "dist/gqless-logger.esm.js",
      "limit": "260 KB"
    }
  ]
}
