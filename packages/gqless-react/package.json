{
  "name": "@dish/gqless-react",
  "version": "2.0.12",
  "license": "MIT",
  "author": "PabloSz",
  "main": "dist/index.js",
  "module": "dist/gqless-react.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "rimraf node_modules/.cache && tsdx build",
    "lint": "tsdx lint",
    "prepare": "rimraf node_modules/.cache && tsdx build",
    "size": "size-limit",
    "start": "tsc-watch -m commonjs --noClear",
    "start:with:example": "tsc-watch -m commonjs --noClear --onSuccess \"pnpm -r --filter gqless-react-example dev\"",
    "test": "tsdx test",
    "test:watch": "tsdx test --watch"
  },
  "dependencies": {
    "react-ssr-prepass": "^1.3.0"
  },
  "devDependencies": {
    "@dish/gqless": "workspace:^3.0.12",
    "@size-limit/preset-small-lib": "^4.9.2",
    "@testing-library/react": "^11.2.5",
    "@testing-library/react-hooks": "^5.1.0",
    "@types/lodash": "^4.14.168",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "graphql": "^15.5.0",
    "kill-port": "^1.6.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-test-renderer": "^17.0.1",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "^3.0.2",
    "size-limit": "^4.9.2",
    "test-utils": "workspace:^0.1.0",
    "tsc-watch": "^4.2.9",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.2"
  },
  "peerDependencies": {
    "@dish/gqless": "workspace:^3.0.0",
    "graphql": "^15.4.0",
    "react": ">=16"
  },
  "engines": {
    "node": ">=10"
  },
  "size-limit": [
    {
      "path": "dist/gqless-react.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/gqless-react.esm.js",
      "limit": "10 KB"
    }
  ]
}
