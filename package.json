{
  "name": "gqless_refactor",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "pnpm build -r --stream",
    "dev": "nodemon",
    "dev:nodemon": "pnpm prepare -r --no-bail && pnpm test -r --no-bail --no-sort --stream",
    "publish:next": "pnpm build && pnpm test && pnpm -r publish --access public --tag next",
    "test": "pnpm test --filter=!gqless_refactor -r --no-sort --no-bail --stream -- --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "nodemon": "^2.0.6"
  },
  "devDependencies": {
    "husky": "^4.3.0",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0"
  }
}
